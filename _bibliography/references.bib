	pages = {51826--51838},
	file = {Lacerda et al. - 2022 - Phasor Modeling Approaches and Simulation Guidelin.pdf:files/682/Lacerda et al. - 2022 - Phasor Modeling Approaches and Simulation Guidelin.pdf:application/pdf},
}

@misc{misyris_zero-inertia_2020,
