	urldate = {2023-05-15},
	booktitle = {20th {International} {Workshop} on {Large}-{Scale} {Integration} of {Wind} {Power} into {Power} {Systems} as well as on {Transmission} {Networks} for {Offshore} {Wind} {Power} {Plants} ({WIW} 2021)},
	publisher = {Institution of Engineering and Technology},
	author = {Cova Acosta, M. A. and Magalhães Soares, B. M. and Abildgaard, H. and Rodrigues Parrini, D. and Shattuck, A. and Cristiano Da Costa, I. and da Rosa, T. D. C. R.},
	year = {2021},
	pages = {503--508},
	file = {Cova Acosta et al. - 2021 - Wind power plant modelling benchmark of RMS vs EMT.pdf:files/681/Cova Acosta et al. - 2021 - Wind power plant modelling benchmark of RMS vs EMT.pdf:application/pdf},
}

@article{lacerda_phasor_2022,
